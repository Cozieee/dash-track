{
  "manifest_version": 2,
  "name": "DashTrack",
  "description": "Track and log your favorite restaurants on DoorDash and eventually others",
  "version": "1.0.0",
  "icons": { "128": "icon-128.png" },
  "content_scripts": [
    {
      "matches": ["https://www.doordash.com/store/*"],
      "css": ["experiments/thirdParty/bootstrap.min.css"],
      "js": [
        "experiments/thirdParty/jquery-3.5.1.min.js",
        "experiments/thirdParty/bootstrap.min.js",
        "experiments/favorite-button/content-script.js"
      ]
    }
  ],
  "permissions": ["activeTab"]
}
